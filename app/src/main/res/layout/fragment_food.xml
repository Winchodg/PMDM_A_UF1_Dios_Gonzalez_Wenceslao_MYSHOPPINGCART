<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FoodFragment">


    <ImageButton
        android:id="@+id/frutas_verduras"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:scaleType="centerCrop"
        android:src="@drawable/frutas_verduras"
        app:layout_constraintBottom_toTopOf="@+id/carne"
        app:layout_constraintEnd_toStartOf="@+id/pan"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/pan"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:scaleType="centerCrop"
        android:src="@drawable/pan"
        app:layout_constraintBottom_toTopOf="@+id/pescado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/frutas_verduras"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/carne"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:scaleType="centerCrop"
        android:src="@drawable/carne"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pescado"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frutas_verduras" />

    <ImageButton
        android:id="@+id/pescado"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/pescado"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/carne"
        app:layout_constraintTop_toBottomOf="@+id/pan" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fruits_vegetables"
        android:fontFamily="sans-serif-black"
        app:layout_constraintBottom_toTopOf="@+id/frutas_verduras"
        app:layout_constraintEnd_toEndOf="@+id/frutas_verduras"
        app:layout_constraintStart_toStartOf="@+id/frutas_verduras" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bread"
        android:fontFamily="sans-serif-black"
        app:layout_constraintBottom_toTopOf="@+id/pan"
        app:layout_constraintEnd_toEndOf="@+id/pan"
        app:layout_constraintStart_toStartOf="@+id/pan" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/meat"
        android:fontFamily="sans-serif-black"
        app:layout_constraintBottom_toTopOf="@+id/carne"
        app:layout_constraintEnd_toEndOf="@+id/carne"
        app:layout_constraintStart_toStartOf="@+id/carne" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fish"
        android:fontFamily="sans-serif-black"
        app:layout_constraintBottom_toTopOf="@+id/pescado"
        app:layout_constraintEnd_toEndOf="@+id/pescado"
        app:layout_constraintStart_toStartOf="@+id/pescado" />

</androidx.constraintlayout.widget.ConstraintLayout>